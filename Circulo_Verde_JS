function setup() {
    // Cria um canvas de 400x400 pixels
    createCanvas(400, 400);
}

function draw() {
    // Define a cor de fundo
    background(220);

    // Define a cor de preenchimento do círculo
    fill(100, 200, 100);

    // Define a cor da borda do círculo
    stroke(0);
    strokeWeight(4);

    // Desenha o círculo no centro do canvas
    ellipse(width / 2, height / 2, 200, 200);
}
